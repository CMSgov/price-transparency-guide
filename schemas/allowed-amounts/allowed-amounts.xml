<?xml version="1.0" encoding="UTF-8" ?>
<_schema>http://json-schema.org/draft-07/schema#</_schema>
<definitions>
    <out_of_network>
        <type>object</type>
        <properties>
            <name>
                <type>string</type>
            </name>
            <billing_code_type>
                <enum>CPT</enum>
                <enum>HCPCS</enum>
                <enum>ICD</enum>
                <enum>DRG</enum>
            </billing_code_type>
            <billing_code_type_version>
                <type>string</type>
            </billing_code_type_version>
            <billing_code>
                <type>string</type>
            </billing_code>
            <description>
                <type>string</type>
            </description>
            <allowed_amounts>
                <type>array</type>
                <items>
                    <_ref>#/definitions/allowed_amounts</_ref>
                </items>
            </allowed_amounts>
        </properties>
        <required>name</required>
        <required>billing_code_type</required>
        <required>billing_code_type_version</required>
        <required>billing_code</required>
        <required>allowed_amounts</required>
        <required>description</required>
    </out_of_network>
    <allowed_amounts>
        <type>object</type>
        <properties>
            <tin>
                <type>object</type>
                <properties>
                   <type>
                       <type>string</type>
                       <enum>ein</enum>
                       <enum>npi</enum>
                   </type>
                   <value>
                       <type>string</type>
                   </value>
                </properties>
                <required>type</required>
                <required>value</required>
            </tin>
            <service_code>
                <type>array</type>
                <_ref>https://www.cms.gov/Medicare/Coding/place-of-service-codes/Place_of_Service_Code_Set</_ref>
            </service_code>
            <payments>
                <type>array</type>
                <items>
                    <_ref>#/definitions/payments</_ref>
                </items>
            </payments>
        </properties>
        <required>tin</required>
        <required>service_code</required>
        <required>payments</required>
    </allowed_amounts>
    <payments>
        <type>object</type>
        <properties>
            <allowed_amount>
                <type>number</type>
            </allowed_amount>
            <providers>
                <type>array</type>
                <items>
                    <_ref>#/definitions/providers</_ref>
                </items>
            </providers>
        </properties>
        <required>allowed_amount</required>
        <required>providers</required>
    </payments>
    <providers>
        <type>object</type>
        <properties>
            <billed_charge>
                <type>number</type>
            </billed_charge>
            <npi>
                <type>array</type>
                <items>
                    <type>number</type>
                </items>
                <uniqueItems>true</uniqueItems>
            </npi>
        </properties>
        <required>billed_charge</required>
        <required>npi</required>
    </providers>
    <reporting_plans>
        <type>object</type>
        <properties>
            <plan_name>
                <type>string</type>
            </plan_name>
            <plan_id_type>
                <type>string</type>
            </plan_id_type>
            <plan_id>
                <type>string</type>
            </plan_id>
            <plan_market_type>
                <enum>group</enum>
                <enum>individual</enum>
            </plan_market_type>
        </properties>
        <required>plan_name</required>
        <required>plan_id_type</required>
        <required>plan_id</required>
        <required>plan_market_type</required>
    </reporting_plans>
</definitions>
<type>object</type>
<properties>
    <reporting_entity_name>
        <type>string</type>
    </reporting_entity_name>
    <reporting_entity_type>
        <type>string</type>
    </reporting_entity_type>
    <reporting_plans>
       <type>array</type>
       <items>
           <_ref>#/definitions/reporting_plans</_ref>
       </items>
    </reporting_plans>
    <last_update_on>
        <type>string</type>
        <format>date</format>
    </last_update_on>
    <version>
        <type>string</type>
    </version>
    <out_of_network>
        <type>array</type>
        <items>
            <_ref>#/definitions/out_of_network</_ref>
        </items>
    </out_of_network>
</properties>
<required>reporting_entity_name</required>
<required>reporting_entity_type</required>
<required>last_update_on</required>

