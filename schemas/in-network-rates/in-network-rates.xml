<?xml version="1.0" encoding="UTF-8" ?>
<_schema>http://json-schema.org/draft-06/schema#</_schema>
<definitions>
    <in_network>
        <type>object</type>
        <properties>
            <negotiation_arrangement>
                <enum>ffs</enum>
                <enum>bundle</enum>
                <enum>capitation</enum>
            </negotiation_arrangement>
            <name>
                <type>string</type>
            </name>
            <billing_code_type>
                <enum>CPT</enum>
                <enum>HCPCS</enum>
                <enum>ICD</enum>
                <enum>MS-DRG</enum>
                <enum>R-DRG</enum>
                <enum>S-DRG</enum>
                <enum>APS-DRG</enum>
                <enum>AP-DRG</enum>
                <enum>APR-DRG</enum>
                <enum>APC</enum>
                <enum>NDC</enum>
                <enum>HIPPS</enum>
                <enum>LOCAL</enum>
                <enum>EAPG</enum>
                <enum>CDT</enum>
            </billing_code_type>
            <billing_code_type_version>
                <type>string</type>
            </billing_code_type_version>
            <billing_code>
                <type>string</type>
            </billing_code>
            <description>
                <type>string</type>
            </description>
            <negotiated_rates>
                <type>array</type>
                <_ref>#/definitions/negotiated_rates</_ref>
            </negotiated_rates>
        </properties>
        <required>negotiation_arrangement</required>
        <required>name</required>
        <required>billing_code_type</required>
        <required>billing_code_type_version</required>
        <required>billing_code</required>
        <required>negotiated_rates</required>
    </in_network>
    <negotiated_rates>
        <type>object</type>
        <properties>
            <providers>
                <type>array</type>
                <_ref>#/definitions/providers</_ref>
                <uniqueItems>true</uniqueItems>
            </providers>
            <negotiated_price>
                <_ref>#/definitions/negotiated_price</_ref>
            </negotiated_price>
       </properties>
        <required>providers</required>
        <required>negotiated_price</required>
    </negotiated_rates>
    <providers> 
      <type>object</type>
      <properties>
        <npi>
          <type>array</type>
            <items>
              <type>number</type>
            </items>
            <uniqueItems>true</uniqueItems>
        </npi>
        <tin>
            <type>string</type>
        </tin>
        <required>npi</required>
        <required>tin</required>
      </properties>
    </providers> 
    <negotiated_price>
        <type>object</type>
        <properties>
            <service_code>
                <type>array</type>
                <_ref>https://www.cms.gov/Medicare/Coding/place-of-service-codes/Place_of_Service_Code_Set</_ref>
                <uniqueItems>true</uniqueItems>
            </service_code>
            <negotiated_type>
                <enum>negotiated</enum>
                <enum>derived</enum>
                <enum>fee schedule</enum>
            </negotiated_type>
            <negotiated_rate>
                <type>number</type>
            </negotiated_rate>
            <expiration_date>
                <type>string</type>
                <format>date</format>
            </expiration_date>
        </properties>
        <required>service_code</required>
        <required>negotiated_type</required>
        <required>negotiated_rate</required>
        <required>expiration_date</required>
    </negotiated_price>
    <reporting_plans>
        <type>object</type>
        <properties>
            <plan_name>
                <type>string</type>
            </plan_name>
            <plan_id_type>
                <type>string</type>
            </plan_id_type>
            <plan_id>
                <type>string</type>
            </plan_id>
            <plan_market_type>
                <enum>group</enum>
                <enum>individual</enum>
            </plan_market_type>
        </properties>
        <required>plan_name</required>
        <required>plan_id_type</required>
        <required>plan_id</required>
        <required>plan_market_type</required>
    </reporting_plans>
</definitions>

<type>object</type>
<properties>
    <reporting_entity_name>
        <type>string</type>
    </reporting_entity_name>
    <reporting_entity_type>
        <type>string</type>
    </reporting_entity_type>
    <last_updated_on>
        <type>string</type>
        <format>date</format>
    </last_updated_on>
    <version>
        <type>string</type>
    </version>
    <reporting_plans>
        <type>array</type>
        <items>
            <_ref>#/definitions/reporting_plans</_ref>
        </items>
    </reporting_plans>
    <in_network>
        <type>array</type>
        <items>
            <_ref>#/definitions/in_network</_ref>
        </items>
    </in_network>
</properties>
<required>reporting_entity_name</required>
<required>reporting_entity_type</required>
<required>last_updated_on</required>

